macro_show_windows: macro_show_windows{
compatible = "zmk,behavior-macro";
label = "macro_show_windows";
#binding-cells = <0>;
bindings = <&kt LCTRL>, <&kp UP_ARROW>, <&kt LCTRL>;
};
macro_lock_mac: macro_lock_mac{
compatible = "zmk,behavior-macro";
label = "macro_lock_mac";
#binding-cells = <0>;
bindings = <&kt LEFT_COMMAND>, <&kt LEFT_CONTROL>, <&kp Q>, <&kt LEFT_CONTROL>, <&kt LEFT_COMMAND>;
};